import 'dart:io';

double area(int b, int h) {
  double a;
  a = 0.5 * b * h;
  return a;
}

main() {
  double a;

  stdout.write("Base : ");
  int base = int.tryParse(stdin.readLineSync()!) ?? 1;

  stdout.write("Height : ");
  int height = int.tryParse(stdin.readLineSync()!) ?? 1;

  a = area(base, height);

  print("Area : $a");
}
